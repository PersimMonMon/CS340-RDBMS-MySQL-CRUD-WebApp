<h1>Actors</h1>
{{! Read table}}
<table>
    <thead>
        {{! For the table header row, we print the DB attribute names }}
        <tr>

            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>

    <tbody>
        {{! For each row, print the attributes }}
        {{#each actors}}
        <tr>
            <td>{{this.firstName}}</td>
            <td>{{this.lastName}}</td>
            <td>{{this.email}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>