<h1>Skits</h1>
{{! Read table}}
<table>
    <thead>
        {{! For the table header row, we print the DB attribute names }}
        <tr>
            <th>Title</th>
            <th>Description</th>
            <th>Show Date</th>
            <th>Author First Name</th>
            <th>Author Last Name</th>
        </tr>
    </thead>

    <tbody>
        {{! For each row, print the attributes }}
        {{#each skits}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.show_date}}</td>
            <td>{{this.firstName}}</td>
            <td>{{this.lastName}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>